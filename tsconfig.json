{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "react-native",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "allowJs": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "isolatedModules": true
    }
}
